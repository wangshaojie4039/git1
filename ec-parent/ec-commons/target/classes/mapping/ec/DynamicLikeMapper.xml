<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="cn.imexue.ec.common.mapper.ec.DynamicLikeMapper" >

	<select id="selectUserNameByDynamicId" resultType="java.lang.String">
		SELECT user_name from t_dynamic_like 
		where dynamic_id=#{dynamicId,jdbcType=BIGINT} 
	</select>


</mapper>